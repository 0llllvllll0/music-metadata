{
  "name": "music-metadata",
  "description": "Streaming music metadata parser for node and the browser.",
  "version": "0.7.2",
  "author": {
    "name": "Borewit",
    "url": "https://github.com/Borewit"
  },
  "contributors": [
    {
      "name": "Lee Treveil",
      "email": "leetreveil@gmail.com",
      "url": "https://github.com/leetreveil"
    }
  ],
  "keywords": [
    "tags",
    "MusicBrainz",
    "Picard",
    "id3",
    "id3v1",
    "id3v2",
    "m4a",
    "mp3",
    "mp4",
    "vorbis",
    "ogg",
    "flac",
    "asf",
    "wma",
    "wmv",
    "tag",
    "ape",
    "MonkeyAudio"
  ],
  "main": "lib",
  "typings": "lib/index",
  "browser": "lib/browser",
  "files": [
    "lib/",
    "dist/"
  ],
  "scripts": {
    "compile": "tsc --pretty",
    "lint": "tslint 'src/**/*.ts' 'test/**/*.ts'",
    "test-browser": "for f in test/test-*.js; do browserify -t brfs $f | testling; done",
    "test": "mocha --require ts-node/register test/*.ts",
    "prepare": "npm run compile",
    "verify": "npm-run-all compile lint test"
  },
  "dependencies": {
    "@types/chai": "^4.0.1",
    "es6-promise": "^4.1.1",
    "filereader-stream": "^1.0.0",
    "fs-extra": "^3.0.1",
    "is-stream": "^1.1.0",
    "strtok3": "^0.2.0",
    "then-read-stream": "^0.1.0",
    "token-types": "0.0.1"
  },
  "devDependencies": {
    "@types/es6-promise": "0.0.32",
    "@types/fs-extra": "^3.0.3",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.6",
    "chai": "^4.0.2",
    "mocha": "^3.4.2",
    "npm-run-all": "^3.1.1",
    "ts-node": "^3.1.0",
    "tslint": "^5.4.3",
    "typescript": "^2.4.1",
    "typings": "2.0.0"
  },
  "testling": {
    "files": [
      "test/test-js-*.js",
      "test/js-*.js"
    ],
    "browsers": [
      "firefox/latest",
      "chrome/latest",
      "safari/latest"
    ]
  },
  "engines": {
    "node": "*"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/borewit/music-metadata.git"
  },
  "jshintConfig": {
    "asi": true,
    "node": true,
    "maxlen": 120,
    "indent": 2
  },
  "license": "MIT",
  "standard": {
    "ignore": [
      "**/dist/**",
      "**/example/**"
    ],
    "globals": [
      "musicMetadata",
      "angular",
      "URL"
    ]
  },
  "bugs": {
    "url": "https://github.com/Borewit/music-metadata/issues"
  }
}
