{
  "name": "music-metadata",
  "description": "Streaming music metadata parser for node and the browser.",
  "version": "0.1.0",
  "author": "Borewit",
  "dependencies": {
    "deep-equal": "^1.0.1",
    "filereader-stream": "^1.0.0",
    "is-stream": "^1.1.0",
    "strtok2": "~1.0.1",
    "through": "~2.3.8"
  },
  "keywords": [
    "tags",
    "MusicBrainz",
    "Picard",
    "id3",
    "id3v1",
    "id3v2",
    "m4a",
    "mp3",
    "mp4",
    "vorbis",
    "ogg",
    "flac",
    "asf",
    "wma",
    "wmv",
    "tag",
    "ape",
    "MonkeyAudio"
  ],
  "main": "lib",
  "browser": "lib/browser",
  "scripts": {
    "pretest": "echo 'Compiling TypeScript...' && tsc && echo 'TypeScript analysis...' && tslint --format=verbose 'lib/**/*.ts'",
    "test": "[ ! -d 'test/' ] && echo 'The test directory is not included with the project due to the size of the test audio files. If you want to run the tests you can git clone the project.' || tape test/test-*.js",
    "test-browser": "for f in test/test-*.js; do browserify -t brfs $f | testling; done",
    "dist": "browserify lib/browser.js --standalone 'music-metadata' -o dist/music-metadata.js"
  },
  "devDependencies": {
    "brfs": "1.2.0",
    "browserify": "^13.0.0",
    "tape": "^4.5.1",
    "testling": "^1.7.1",
    "typings": "^2.0.0"
  },
  "testling": {
    "files": [
      "test/test-js-*.js",
      "test/js-*.js"
    ],
    "browsers": [
      "firefox/latest",
      "chrome/latest",
      "safari/latest"
    ]
  },
  "engines": {
    "node": "*"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/borewit/music-metadata.git"
  },
  "jshintConfig": {
    "asi": true,
    "node": true,
    "maxlen": 120,
    "indent": 2
  },
  "license": "MIT",
  "standard": {
    "ignore": [
      "**/dist/**",
      "**/example/**"
    ],
    "globals": [
      "musicMetadata",
      "angular",
      "URL"
    ]
  },
  "bugs": {
    "url": "https://github.com/Borewit/music-metadata/issues"
  }
}
