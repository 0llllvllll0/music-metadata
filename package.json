{
  "name": "music-metadata",
  "description": "Streaming music metadata parser for node and the browser.",
  "version": "0.8.3",
  "author": {
    "name": "Borewit",
    "url": "https://github.com/Borewit"
  },
  "contributors": [
    {
      "name": "Lee Treveil",
      "email": "leetreveil@gmail.com",
      "url": "https://github.com/leetreveil"
    }
  ],
  "keywords": [
    "tag",
    "tags",
    "MusicBrainz",
    "Picard",
    "IDd3",
    "ID3v1",
    "ID3v2",
    "m4a",
    "mp3",
    "mp4",
    "Vorbis",
    "ogg",
    "flac",
    "asf",
    "wma",
    "wmv",
    "tag",
    "ape",
    "MonkeyAudio",
    "aiff",
    "wav",
    "WavPack"
  ],
  "main": "lib",
  "typings": "lib/index",
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "del-cli lib/** src/**/*.js.map src/**/*.d.ts",
    "compile-src": "tsc -p src",
    "compile-test": "tsc -p test",
    "compile": "npm run compile-src && npm run compile-test",
    "lint": "tslint 'src/**/*.ts' --exclude 'src/**/*.d.ts' 'test/**/*.ts' --exclude 'test/**/*.d.ts'",
    "test": "mocha --require ts-node/register test",
    "prepare": "npm run clean && npm run compile",
    "start": "npm-run-all compile lint cover-test",
    "cover-test": "nyc npm run test",
    "coveralls": "npm run cover-test && nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "es6-promise": "^4.1.1",
    "fs-extra": "^4.0.1",
    "strtok3": "^1.3.2",
    "then-read-stream": "^1.0.3",
    "token-types": "^0.9.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/es6-promise": "0.0.32",
    "@types/fs-extra": "^4.0.2",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.0.26",
    "chai": "^4.1.2",
    "coveralls": "^2.13.1",
    "del-cli": "^1.1.0",
    "mocha": "^3.5.3",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.2.1",
    "source-map": "^0.5.7",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2"
  },
  "testling": {
    "files": [
      "test/test-js-*.js",
      "test/js-*.js"
    ],
    "browsers": [
      "firefox/latest",
      "chrome/latest",
      "safari/latest"
    ]
  },
  "engines": {
    "node": "*"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/borewit/music-metadata.git"
  },
  "license": "MIT",
  "standard": {
    "ignore": [
      "**/dist/**",
      "**/example/**"
    ],
    "globals": [
      "musicMetadata",
      "angular",
      "URL"
    ]
  },
  "bugs": {
    "url": "https://github.com/Borewit/music-metadata/issues"
  },
  "nyc": {
    "check-coverage": true,
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "coverage"
  }
}
